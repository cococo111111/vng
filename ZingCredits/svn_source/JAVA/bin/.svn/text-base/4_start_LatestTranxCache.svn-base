#!/bin/sh

. ./set_env
PROJECT=LatestTranxCache
JAR=$PROJECT-1.5.jar
PIDFILE=pid_$PROJECT

CURDIR=`pwd`
HOME=$CURDIR/../$PROJECT
CONF=$HOME/conf

java -Dzname=$PROJECT -Dconfpath="$HOME/" -Dconfig="$CONF" -Xmx6g -Djava.rmi.server.hostname=$HOST -Dcom.sun.management.jmxremote.port=9876 -Dcom.sun.management.jmxremote.ssl=false -Dcom.sun.management.jmxremote.authenticate=false -jar "$DEPLOY_DIR/$JAR" &
/bin/echo -n $! > "$PIDFILE"

