@charset "utf-8";

/*********** layout_mexu ***************/
#layout_mexu { width:auto; height:auto}
#layout_mexu .clear { clear:both;}
#layout_mexu .left_mexu { float:left; background:#f2f4f7; width:166px; height:auto; margin-right:20px; padding:7px;}
#layout_mexu .avatar { float:left; width:50px; height:70px;}
#layout_mexu .desc { float:right; width:106px; height:70px;}
#layout_mexu .desc p.name { margin-bottom:5px; color:#476da4; font-size:12px; font-weight:bold; line-height:16px;}
#layout_mexu .left_mexu .desc p.name a { color:#476da4;}
#layout_mexu .left_mexu .desc p.gohome { color:#476da4; font-size:11px;}
#layout_mexu .left_mexu .desc p.gohome a { background:url(../images/imgmexu_01.gif) left 1px no-repeat; padding-left:14px;}
#layout_mexu .left_mexu ul.subcat_mexu { list-style:none;}
#layout_mexu .left_mexu ul.subcat_mexu li { height:24px;color:#555;line-height:24px; border-bottom:1px solid #dedede;}
#layout_mexu .left_mexu ul.subcat_mexu li a { display:block; height:24px; padding-left:7px; line-height:24px; background:none; color:#555; text-decoration:none;}
#layout_mexu .left_mexu ul.subcat_mexu li a:hover, #layout_mexu .left_mexu ul.subcat_mexu li.active a { background:#6e9dd1; color:#fff; text-decoration:none;}
#layout_mexu .right_mexu { float:left; background:#fff; width:100%; height:auto;}
#layout_mexu .right_mexu .box_mexu { background:#f2f4f7; width:99%; margin-top:15px; padding:5px; border:1px solid #c9d5e2;}
#layout_mexu .right_mexu .box_mexu .row_mexu { height:27px; clear:both;}
#layout_mexu .right_mexu .box_mexu .row_mexu strong.spec { color:#6E9DD1;font-weight:bold; font-size:12px;}
#layout_mexu .right_mexu .box_mexu .row_mexu .txt_mexu { float:left; width:141px; margin-right:10px;}
#layout_mexu .right_mexu .box_mexu .row_mexu .input_mexu { float:left;}
#layout_mexu .right_mexu .box_mexu .row_mexu .input_mexu img {margin-left: 3px;margin-top: 3px;}
#layout_mexu .right_mexu .box_mexu .row_mexu .input_mexu select { display:inline; background:#fff; float:left; padding:3px; color:#555; font-size:11px; font-family:Tahoma, Helvetica, sans-serif; border:1px solid #a8acad;}
#layout_mexu .right_mexu .box_mexu .row_mexu .input_mexu input { display:inline; background:#fff; float:left; padding:3px; color:#555; font-size:11px; font-family:Tahoma, Helvetica, sans-serif; border:1px solid #a8acad;}
#layout_mexu .right_mexu .box_mexu .row_mexu .input_mexu input.button { display:inline; background:none; float:left; margin-left:5px; padding:0; border:0;}
#layout_mexu .right_mexu .box_mexu .row_mexu .input_mexu input.im1 { width:90px;}
.table_mexu { background:#fff; margin-top:10px; padding:1px;width:100%}
.table_mexu table { margin:0;}
.table_mexu table table { margin-bottom:-1px;}
.table_mexu table td { text-align:center; height:26px; border-bottom:1px solid #e7e7e7; border-right:1px solid #e7e7e7;}
.table_mexu table .lstitle { background:#486fa6; height:26px; text-align:center; color:#fff; font-size:11px; font-weight:bold;}
.table_mexu table .lssecond { background:#f6f6f6;}
.loadframe{
width:800px;margin:3px;
}
.table_mexu table .blank { background:#f2f4f7;}
