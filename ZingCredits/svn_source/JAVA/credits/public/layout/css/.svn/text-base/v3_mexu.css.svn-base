    margin: 0;
    padding: 0;
}
.bl_right {
    background-image: url("http://css.me.zdn.vn/images/zme_btmrightblog.gif");
    background-position: right bottom;
    background-repeat: no-repeat;
    display: block;
    float: right;
    height: 3px;
    margin: 0;
    padding: 0;
    width: 3px;
}
.bl_left {
    background-image: url("http://css.me.zdn.vn/images/zme_btmleftblog.gif");
    background-position: left bottom;
    background-repeat: no-repeat;
    display: block;
    float: left;
    height: 3px;
    margin: 0;
    padding: 0;
    width: 3px;
}
