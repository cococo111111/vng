
USE zingcredits_app;

CREATE TABLE  `zingcredits_app`.`remain_balance` (
  `analyticDate` timestamp NOT NULL default CURRENT_TIMESTAMP,
  `remainBalance` double NOT NULL, -- số tiền tồn của ngày
  `note` varchar(1000) , -- note
   PRIMARY KEY (`analyticDate`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8;

